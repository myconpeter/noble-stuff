                        // Variable to store the interval ID
                        let intervalId;
                        // con_status function
                        function con_status(id) {
                            // Target the element by its id
                            const statusElement = document.getElementById('con_stats');
                            //console.log(statusElement);

                            // Array of words to cycle through
                            const words = [`Connecting to ${id} wallet...`, `Processing to Wallet...`, 'Loading...'];

                            // Delay in milliseconds between word changes
                            const delay = 2000; // 2000 milliseconds = 2 seconds

                            // Function to rotate through the words
                            function rotateWords() {
                                let currentIndex = 0;

                                // Set initial word
                                statusElement.textContent = words[currentIndex];

                                // Rotate words at the specified interval
                                intervalId = setInterval(() => {
                                    currentIndex = (currentIndex + 1) % words.length;
                                    statusElement.textContent = words[currentIndex];
                                    $("#con_stats").css("color", "");
                                    $("#con_button").css("display", "none");
                                    $(".loader").css("display", "inline-flex");
                                    $("#input_con").css("display", "none");

                                    // Clear the interval after the last word
                                    if (currentIndex === words.length - 1) {
                                        clearInterval(intervalId);
                                    }

                                }, delay);
                            }

                            // Start rotating the words
                            rotateWords();
                        }

                        // reset modal after every exit
                        $('.modal_close').click(function() {
                            // $("#con_stats").html("Connecting...");
                            $("#con_button").css("display", "none");
                            $("#img").css("display", "none");
                            //    // $(".loader").css("display", "inline-flex");
                            clearInterval(intervalId);

                            //     $("#con_stats").css("display", "inline-flex");
                            $("#input_con").css("display", "none");
                            $("#con_stats").css("color", "");
                            //     $(".sendp").trigger("reset");
                            //alert("jkygygy");
                            // Clear the interval 

                        })

